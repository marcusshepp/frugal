<% if current_user %>

<% @categories.each do |category| %>
<table>
  <tr>
    <th>Category</th>
<tr>
  <td><%= category.name %></td>
  <td><%= link_to 'Edit', edit_category_path(category) %></td>
</tr>
<% end %>
</table>
<%= link_to 'Create Category', new_category_path %>
<% end %>
<% if not current_user %>
<%= render 'sessions/new.html.erb' %>
<% end %>
